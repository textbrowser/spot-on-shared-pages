        Iterate('onLoad', (survey) => {
            var elSurveyPlaceholder = document.getElementById("survey-placeholder");
            elSurveyPlaceholder.innerHTML = "<button onclick='window.launchSurvey()'>Were they helpful?</button>";

            window.launchSurvey = function() {
