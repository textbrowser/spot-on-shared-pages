<section id="content" role="main">
<!-- start article title -->
<header class='titles'><h1>Of Unicorns: On My Little Pony</h1></header><!-- end article title -->
<div class='article-body-wrap'>
	<!-- start article subtitles -->
	<div class='article-subtitles'>
		<section class='article-top-rail'>
			<address>By <a href="https://www.theparisreview.org/blog/author/lives/" title="Posts by Lucy Ives" rel="author">Lucy Ives</a></address>
			<time>September 9, 2024</time>
			<h3 class='h6'><a href="https://www.theparisreview.org/blog/on-things/" rel="category tag">On Things</a></h3>
		</section>
    <!-- <p>The art and life of Mark di Suvero</p> -->
	</div>
	<!-- end article subtitles -->

