// Start Iterate: this will check the server to see if a survey should be displayed
Iterate('install')

// Pass custom properties to Iterate: the 2nd parameter is user-level data, the 3rd parameter is response-level data
var url = new URL(window.location.href);
Iterate('identify', null, {
    canonical: 'https://www.propublica.org/article/formaldehyde-epa-trump-public-health-danger',
    utm_campaign: url.searchParams.get("utm_campaign"),
    utm_medium: url.searchParams.get("utm_medium"),
    utm_source: url.searchParams.get("utm_source")
})

</script>

<!-- End Iterate Code -->

    
