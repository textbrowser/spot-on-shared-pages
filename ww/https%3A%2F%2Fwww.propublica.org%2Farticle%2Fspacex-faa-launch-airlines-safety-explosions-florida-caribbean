    // set up global variables first
    let isMobile = window.innerWidth < 750;
    function getColorScheme() {
        let colorScheme;

        // Detect system preference
        if (window.matchMedia && window.matchMedia('(prefers-color-scheme: dark)').matches) {
            colorScheme = "dark";
        } else {
            colorScheme = "light";
        }

        // Check <body> classes and override colorScheme if necessary
        const bodyHtml = document.documentElement;
        if (bodyHtml.classList.contains('force-dark-mode')) {
            colorScheme = "dark";
        } else if (bodyHtml.classList.contains('force-light-mode')) {
            colorScheme = "light";
        }

        return colorScheme;
    }

    function lockVH() {
        const vh = window.visualViewport
            ? window.visualViewport.height
            : window.innerHeight;

        document.documentElement.style.setProperty(
            '--vh',
            `${vh * 0.01}px`
        );

        // console.log("locked vh =" + vh);
    }

    // Wait until load + a frame
    window.addEventListener('load', () => {
        requestAnimationFrame(() => {
            requestAnimationFrame(lockVH);
        });
    });

