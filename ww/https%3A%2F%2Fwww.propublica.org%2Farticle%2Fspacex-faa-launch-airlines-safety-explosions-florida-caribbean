            return a + (b - a) * t;
        }

        const debounce = (fn, delay) => {
            let timeout;
            return (...args) => {
                clearTimeout(timeout);
                timeout = setTimeout(() => fn(...args), delay);
            };
        };

        // adjust scrim opacity only if the user is not on mobile

        function setUpScrim() {
            gsap.registerPlugin(ScrollTrigger);

             if (!isMobile) {

                gsap.fromTo(
                    "#mapScrim",
                    { opacity: 0 }, // Start value
                    {
                        opacity: 1,   // End value
                        ease: "none",
                        scrollTrigger: {
                        trigger: "#step-4",
                        start: "top 50%", // Start fading in
                        end: "top top",   // Fully visible
                        scrub: true,
                        immediateRender: false,
                        invalidateOnRefresh: true,
                        }
                    }
                );
            }
            ScrollTrigger.refresh();
        }

        setUpScrim();

        if (!isMobile) {
                window.addEventListener('resize', debounce(() => {
                    flightMapLoad();
                }, 100));
            }

        ///// FLIGHT MAP LOAD FUNCTION
        function flightMapLoad() {

            let labelsLoaded = false;
            const container = flightMap.getContainer();
            const offscreen = new OffscreenCanvas(1, 1);
            const offCtx = offscreen.getContext('2d', { alpha: true });

            const canvas = document.createElement('canvas');
            const dpr = window.devicePixelRatio || 1;
            const onscreenCtx = canvas.getContext('bitmaprenderer', { desynchronized: false });

            function resizeCanvasToContainer() {
                requestAnimationFrame(() => {
                    const bounds = container.getBoundingClientRect();

                    if (bounds.height === 0) {
                        // Try again on next frame
                        requestAnimationFrame(resizeCanvasToContainer);
                        return;
                    }

                    const width = Math.round(bounds.width * dpr);
                    const height = Math.round(bounds.height * dpr);

                    // Backing resolutions
                    canvas.width = width;
                    canvas.height = height;
                    offscreen.width = width;
                    offscreen.height = height;

                    // CSS size
                    canvas.style.width = bounds.width + "px";
                    canvas.style.height = bounds.height + "px";

                    // IMPORTANT: no DPR transform here
                    offCtx.setTransform(1, 0, 0, 1, 0, 0);
                    offCtx.imageSmoothingEnabled = false;
                });
            }

            resizeCanvasToContainer();

            canvas.style.position = 'absolute';
            canvas.style.top = 0;
            canvas.style.left = 0;
            canvas.style.pointerEvents = 'none';
            canvas.style.zIndex = "100";
            container.appendChild(canvas);

            // Keep canvas in sync with map size

            if (!isMobile) {
                flightMap.on('resize', resizeCanvasToContainer);
            }

            const mapSvg = d3.select(container).append('svg')
                .attr('class', 'd3-overlay')
                .style('position', 'absolute')
                .style('top', 0)
                .style('left', 0)
                .style('width', '100%')
                .style('height', '100%');

            const g = mapSvg.append('g');

            // load data for DAL573
            d3.csv('https://static.propublica.org/projects/graphics/2025-spacex-flights/dal573_partial_trace.csv', d => {
                const latitude = +d.lat;
                const longitude = +d.lon;
                const heading = +d.heading;
                const time = new Date(d.time);

                return (!isNaN(latitude) && !isNaN(longitude) && !isNaN(heading) && !isNaN(time))
                    ? { plane_id: d.icao24, latitude, longitude, heading, time }
                    : null;

            }).then(data => {
                data = data.filter(Boolean).sort((a, b) => a.time - b.time);
                const coords = data.map(d => [d.longitude, d.latitude]);
                let projectedCoords = coords.map(d => flightMap.project(d));

                function updateProjectedCoords() {
                    projectedCoords = coords.map(d => flightMap.project(d));
                }
                flightMap.on('move', updateProjectedCoords);
                flightMap.on('zoom', updateProjectedCoords);
                if (!isMobile) {
                    flightMap.on('resize', updateProjectedCoords);
                }

                // Dummy object to animate through time (0–100)
                const progressObj = { value: 0 };
                const planeImg = new Image();
                planeImg.src = planeSrc;

                planeImg.onload = () => {
                // Now that the plane image is loaded, simulate a scroll event
                    window.dispatchEvent(new Event('scroll'));

                    setTimeout(() => {
                        updateFlightVisuals(); // draws the plane immediately
                    }, 10);

                };

                // set up line
                const lineGenerator = d3.line()
                    .x(d => flightMap.project([d[0], d[1]]).x)
                    .y(d => flightMap.project([d[0], d[1]]).y);

                // append path
                const path = g.append("path")
                    .datum(coords)
                    .attr("id", "plane-path")
                    .attr("d", lineGenerator)
                    .attr("fill", "none")
                    .attr("stroke", planeColor)
                    .attr("stroke-width", 2);

                // get total length of the line

                const totalLength = path.node().getTotalLength();

                path
                    .attr("stroke-dasharray", totalLength + " " + totalLength)
                    .attr("stroke-dashoffset", totalLength);

                const flightState = { p: 0 };

                const flightTl = gsap.to(flightState, {
                    p: 1,
                    duration: 1,
                    ease: "none",
                    paused: true,
                    onUpdate: () => {
                        updateFlightVisuals();
                    }
                });

                let pathOpacity = 1;

                function drawFlightPath(progressFraction) {
                    const n = Math.floor(progressFraction * projectedCoords.length);

                    offCtx.save();
                    offCtx.clearRect(0, 0, offscreen.width, offscreen.height);

                    // Apply opacity
                    offCtx.globalAlpha = pathOpacity;

                    // Draw flight path
                    offCtx.beginPath();
                    offCtx.lineWidth = 2 * dpr;
                    offCtx.strokeStyle = planeColor;

                    if (n > 0) {
                        offCtx.moveTo(projectedCoords[0].x * dpr, projectedCoords[0].y * dpr);
                        for (let i = 1; i < n; i++) {
                            offCtx.lineTo(projectedCoords[i].x * dpr, projectedCoords[i].y * dpr);
                        }
                        offCtx.stroke();
                    }

                    offCtx.restore();
                }

                // store final plane coords

                let lastPlaneCoords = {
                    longitude: data[data.length - 1].longitude,
                    latitude: data[data.length - 1].latitude,
                    heading: (data[data.length - 1].heading - 180)
                };

                // console.log(lastPlaneCoords.heading);

                let flightFinished = false;

                let planeOpacity = 1;

                function drawPlaneFromPixels(x, y, heading) {
                     offCtx.imageSmoothingEnabled = false;

                    const px = Math.round(x * dpr);
                    const py = Math.round(y * dpr);

                    const size = 24 * dpr;

                    offCtx.save();
                    offCtx.globalAlpha = planeOpacity;
                    offCtx.translate(px, py);
                    offCtx.rotate((heading + 45) * Math.PI / 180);
                    offCtx.drawImage(
                        planeImg,
                        -size / 2,
                        -size / 2,
                        size,
                        size
                    );
                    offCtx.restore();
                }

                const firstTime = data[0].time;
                const lastTime = data[data.length - 1].time;

                data.forEach((d, i) => {
                    d.pathFraction = i / (data.length - 1); // already done
                    d.timeFraction = (d.time - firstTime) / (lastTime - firstTime); // normalize to 0→1
                });

                let lastValidHeading = data[0].heading - 180;

                function updateFlightVisuals() {
                    const progressFraction = flightTl.progress(); // 0 → 1

                    // Determine the current point along the flight path
                    const n = Math.min(Math.floor(progressFraction * projectedCoords.length), projectedCoords.length - 1);
                    const coord = projectedCoords[n];

                    // Project the current coordinate to canvas pixels
                    const x = coord.x;
                    const y = coord.y;

                    // Sample a second point slightly ahead for heading calculation
                    const nextN = Math.min(n + 1, projectedCoords.length - 1);
                    const nextCoord = projectedCoords[nextN];

                    let heading = lastValidHeading

                    if (nextCoord) {
                        const nextX = nextCoord.x;
                        const nextY = nextCoord.y;

                        const dx = nextX - x;
                        const dy = nextY - y;

                        if (dx !== 0 || dy !== 0) {
                            heading = Math.atan2(dy, dx) * 180 / Math.PI;
                            lastValidHeading = heading; // update last valid heading
                        }
                    }

                    // Draw progressive flight path
                    drawFlightPath(progressFraction);

                    // Draw plane at current position
                    if (planeImg.complete) {
                        drawPlaneFromPixels(x, y, heading);
                    }

                    // --- Update time label ---
                    const i = d3.bisector(d => d.pathFraction).right(data, progressFraction);
                    const prevPoint = data[i - 1];
                    const nextPoint = data[i];
                    let timeValue;

                    if (prevPoint && nextPoint) {
                        const t = (progressFraction - prevPoint.pathFraction) / (nextPoint.pathFraction - prevPoint.pathFraction);
                        const prevTime = prevPoint.time.getTime();
                        const nextTime = nextPoint.time.getTime();
                        const interpolatedTime = prevTime + t * (nextTime - prevTime);
                        timeValue = new Date(interpolatedTime);
                    } else if (prevPoint) {
                        timeValue = prevPoint.time;
                    }

                    if (timeValue) {

                        let formatted = timeValue.toLocaleString('en-US', {
                            timeZone: 'America/New_York',
                            hour12: true,
                            hour: 'numeric',
                            minute: '2-digit'
                        });

                        // Replace "AM"/"PM" with "a.m."/"p.m."
                        formatted = formatted.replace("AM", "a.m.").replace("PM", "p.m.");

                        formatted = formatted + " EST";

                        d3.select('#time-label').text(formatted);
                    }

                    const bitmap = offscreen.transferToImageBitmap();
                    onscreenCtx.transferFromImageBitmap(bitmap);
                }

                //set up scrollama

                const scrolly = document.querySelector(".scrolly-opener");
                const scrollySteps = scrolly.querySelectorAll(".step");

                const scrollContainer =
                    document.scrollingElement || document.documentElement;

                const scroller = scrollama();

                scroller
                    .setup({
                        step: scrollySteps,
                        offset: 0.78,
                        progress: true,
                        container: scrollContainer
                    })
                    .onStepEnter(handleStepEnter)
                    .onStepProgress(handleStepProgress)

                if (!isMobile) {
                    window.addEventListener('resize', scroller.resize);
                }

                // create flightWasForced to control the plane's position if the page is laoded below the opener

                let flightWasForced = false;

                function updateMapCameraOnLoad() {
                    const viewportHeight = window.innerHeight;
                    let activeIndex = -1;
                    let activeProgress = 0;

                    scrollySteps.forEach((step, i) => {
                        const rect = step.getBoundingClientRect();
                        const top = rect.top;
                        const bottom = rect.bottom;
                        const targetY = viewportHeight * 0.75;

                        if (top <= targetY && bottom >= targetY) {
                            activeIndex = i;
                            const stepHeight = rect.height;
                            const scrolled = targetY - rect.top;
                            activeProgress = Math.min(Math.max(scrolled / stepHeight, 0), 1);
                        }

                    });

                    if (activeIndex >= 0) {
                        // Step is visible — update plane/timeline
                        handleStepProgress({
                            element: scrollySteps[activeIndex],
                            index: activeIndex,
                            progress: activeProgress
                        });
                    } else if (window.scrollY < 5) {
                        // Top of page — default to first step
                        handleStepProgress({
                            element: scrollySteps[0],
                            index: 0,
                            progress: 0
                        });
                    } else {
                        // Far down — jump to last camera step
                        const lastStep = cameraSteps[cameraSteps.length - 1];
                        flightMap.jumpTo({
                            center: lastStep.end.center,
                            zoom: lastStep.end.zoom,
                            bearing: lastStep.end.bearing,
                        });

                        flightFinished = false;
                        flightTl.progress(FLIGHT_END_PROGRESS);
                        updateFlightVisuals();

                        flightWasForced = true;
                    }
                }

                // Initial call on load
                updateMapCameraOnLoad();

                // Update on resize to keep camera and flight visuals in sync
                if (!isMobile) {
                    window.addEventListener('resize', debounce(() => {
                        drawFlightPath();
                        updateFlightVisuals();
                        updateMapCameraOnLoad();
                    }, 100));
                }

                function handleStepProgress({ element, index, progress }) {

                    const range = stepRanges[index];
                    if (!range) return;

                    let globalProgress;

                    if (range.start === range.end) {
                        // Step has no animation; still update timeline to this fixed progress
                        globalProgress = range.start;
                    } else {
                        // Interpolate progress within this step's range
                        globalProgress = range.start + (range.end - range.start) * progress;
                    }

                    // Update timeline progress for the path — this drives camera animation
                    flightTl.progress(globalProgress / 100);

                    const camIndex = cameraStepIndexMap[index];
                    if (camIndex !== undefined) {

                        const step = cameraSteps[camIndex];
                        const zoom = lerp(step.start.zoom, step.end.zoom, progress);
                        const lng  = lerp(step.start.center[0], step.end.center[0], progress);
                        const lat  = lerp(step.start.center[1], step.end.center[1], progress);
                        const bearing = lerp(step.start.bearing, step.end.bearing, progress);

                        flightMap.jumpTo({
                            center: [lng, lat],
                            zoom: zoom,
                            bearing: bearing,
                        });

                    }

                    // Always update plane visuals on top
                    updateFlightVisuals();
                }


                function handleStepEnter(response) {

                    let stepNum = response.index - 1;
                    var planePath = d3.select("plane-path");

                    if (stepNum <= 2) {
                        flightFinished = false;
                    } else if (stepNum >= 3) {
                        flightFinished = true;
                    }

                    if (stepNum >= 1 && !labelsLoaded) {
                        setUpLabels();
                        labelsLoaded = true;
                    }

                    if (stepNum === 0 && response.direction === "up") {
                        fadeElement("dra-zone-fill-layer", "fill", 0);
                        fadeElement("dra-zone-label-layer", "text", 0);
                    }
                    else if (stepNum === 0 && response.direction === "down") {


                    }
                    else if (stepNum === 1 && response.direction === "down") {
                        setUpLabels();
                        fadeElement("dra-zone-fill-layer", "fill", .4);
                        fadeElement("dra-zone-label-layer", "text", 1);

                    }
                    else if (stepNum === 2 && response.direction === "up") {
                        fadeElement("flights-layer", "line", 0);
                        fadeElement("planes-layer", "icon", 0);
                    }
                    else if (stepNum === 3 && response.direction === "down") {
                        fadeElement("flights-layer", "line", 1);
                        fadeElement("planes-layer", "icon", 1);

                    }
                    else if (stepNum === 4 && response.direction === "down") {
                        document.getElementById("time-label").style.opacity = 0;
                    }
                    else if (stepNum === 4 && response.direction === "up") {
                        document.getElementById("time-label").style.opacity = 1;
                        fadeElement("dra-zone-fill-layer", "fill", .4);
                        fadeElement("dra-zone-label-layer", "text", 1);
                        fadeElement("planes-layer", "icon", 1);

                    }
                };

            });
        };

    // Re-load map and canvas elements if color scheme changes
    function applyColorScheme(newScheme) {
        colorScheme = newScheme;
        colorSchemeVars(); // updates planeSrc, mapStyle, etc.
        flightMap.setStyle(mapStyle); // use updated mapStyle from vars

        renderCanvasElements() // update elements in jan15-flights.html
        flightMap.once('style.load', () => {
            flightMapLoad(); // re-add sources, layers, etc.
        });
    }

    const modeToggle = document.getElementById("dark-mode-toggle__select");

    if (modeToggle) {
        function updateColorScheme(mode) {
            // console.log(mode);
            document.documentElement.classList.remove("force-light-mode", "force-dark-mode");

            if (mode === "light") {
                document.documentElement.classList.add("force-light-mode");
                applyColorScheme("light");
            } else if (mode === "dark") {
                document.documentElement.classList.add("force-dark-mode");
                applyColorScheme("dark");
            } else {
                // Auto mode - use system preference
                const prefersDark = window.matchMedia("(prefers-color-scheme: dark)").matches;
                applyColorScheme(prefersDark ? "dark" : "light");
            }
        }

        // Watch for user interaction
        modeToggle.addEventListener("change", () => {
            updateColorScheme(modeToggle.value);
        });

    }

    // set up fades

    function fadeElement(layerName, layerType, value) {

        flightMap.setPaintProperty(layerName, layerType + "-opacity-transition", {
            duration: 1000,
        });
        flightMap.setPaintProperty(layerName, layerType + "-opacity", value);
    }

    // set up map labels
    function setUpLabels() {

        // console.log("setUpLabels is running");

        if(!flightMap.getSource('dra-zone-source')) {
            flightMap.addSource('dra-zone-source', {
                type: 'geojson',
                data: 'https://static.propublica.org/projects/graphics/2025-spacex-flights/january_restricted.geojson'
            });
        }

        if(!flightMap.getLayer('dra-zone-fill-layer')) {
            flightMap.addLayer({
                id: 'dra-zone-fill-layer',
                type: 'fill',
                source: 'dra-zone-source',
                paint: {
                    'fill-color': faaColor,
                    'fill-opacity': 0,
                }
            });
        }

        if(!flightMap.getSource('dra-zone-label-source')) {
            flightMap.addSource("dra-zone-label-source", {
                type: "geojson",
                data: {
                    type: "FeatureCollection",
                    features: [
                        {
                            type: "Feature",
                            properties: {},
                            geometry: {
                                coordinates: [-70.2, 20.95],
                                type: "Point",
                            },
                        },
                    ],
                },
            });
        }


        if(!flightMap.getLayer('dra-zone-label-layer')) {
            flightMap.addLayer({
                id: "dra-zone-label-layer",
                type: "symbol",
                source: "dra-zone-label-source",
                layout: {
                    "text-field": "FAA's Debris Zone",
                    "text-font": ["Graphik Semibold"],
                    "text-size": 18,
                    "text-letter-spacing": 0,
                    "text-offset": [0, 0],
                    "text-anchor": "center",
                    "text-rotate": 0,
                    "text-allow-overlap": true,
                },
                paint: {
                    "text-color": faaColorLabelDark,
                    "text-opacity": 0,
                    "text-halo-color": colorScheme === 'dark' ? "#1b2127" : "#ffffff",
                    "text-halo-width": .75,

                },
            });
        }


         if(!flightMap.getSource('flights-source')) {
            flightMap.addSource("flights-source", {
                type: "geojson",
                data: "https://static.propublica.org/projects/graphics/2025-spacex-flights/20251203-opener-flight-paths-simplified.geojson",
                lineMetrics: true
            });

        }

        if(!flightMap.getLayer("flights-layer")) {
            flightMap.addLayer({
                id: 'flights-layer',
                type: 'line',
                source: 'flights-source',
                layout: {
                    'line-cap': 'round',
                    'line-join': 'round'
                },
                paint: {
                    'line-width': 2,
                    'line-opacity': 0,
                    // Gradient from start to end along each line
                    'line-gradient': [
                        'interpolate',
                        ['linear'],
                        ['line-progress'],
                        0, colorScheme === 'dark' ? 'rgba(255, 255, 255, 0)' : 'rgba(27, 27, 27, 0)',   // start
                        0.5, colorScheme === 'dark' ? 'rgba(255, 255, 255, .5)' : 'rgba(27, 27, 27, .5)',
                        0.9, colorScheme === 'dark' ? 'rgba(255, 255, 255, 1)' : 'rgba(27, 27, 27, 1)'   // end
                    ]
                }
            })
        }

        if(!flightMap.getSource('planes-source')) {
            flightMap.addSource("planes-source", {
                type: "geojson",
                data: "https://static.propublica.org/projects/graphics/2025-spacex-flights/20251203-opener-end-points.geojson"
            });

        }

        if(!flightMap.getLayer("planes-layer")) {
            flightMap.addLayer({
                id: 'planes-layer',
                type: 'symbol',
                source: 'planes-source',
                layout: {
                    'icon-image': colorScheme === "dark" ? "plane_white_stroke" : "plane_black_stroke",
                    'icon-size': 1,
                    'icon-allow-overlap': true,
                    'icon-ignore-placement': true,
                    'icon-rotate': ['-', ['get', 'angle'], 45],
                    'icon-rotation-alignment': 'map',
                    'icon-keep-upright': false
                },
                paint: {
                    'icon-opacity': 0,
                }
            })
        }

    }

    // SET UP AUDIO

    let audioState = {
        duration: 10.58,
        activeWordId: 0,
        element: undefined,
        interval: undefined,
        json: [{"word":"Due","speaker":"none","start":0,"end":0.3,"id":0},{"word":"to","speaker":"none","start":0.5,"end":0.7,"id":1},{"word":"a","speaker":"none","start":0.7,"end":0.8,"id":2},{"word":"space","speaker":"none","start":0.8,"end":1.1,"id":3},{"word":"vehicle","speaker":"none","start":1.2,"end":1.9,"id":4},{"word":"mishap.","speaker":"none","start":2.3,"end":3,"id":5},{"word":"Rocket","speaker":"none","start":3.2,"end":3.8,"id":6},{"word":"launch","speaker":"none","start":4.2,"end":4.9,"id":7},{"word":"that","speaker":"none","start":5,"end":5.6,"id":8},{"word":"basically","speaker":"none","start":5.7,"end":6.2,"id":9},{"word":"exploded","speaker":"none","start":6.2,"end":6.6,"id":10},{"word":"in","speaker":"none","start":6.6,"end":7.2,"id":11},{"word":"between","speaker":"none","start":7.4,"end":7.8,"id":12},{"word":"our","speaker":"none","start":7.8,"end":8,"id":13},{"word":"airspace","speaker":"none","start":8,"end":8.3,"id":14},{"word":"and","speaker":"none","start":8.3,"end":8.5,"id":15},{"word":"Miami.","speaker":"none","start":8.5,"end":8.9,"id":16},{"word":"I'm","speaker":"none","start":9,"end":9.1,"id":17},{"word":"going","speaker":"none","start":9.1,"end":9.3,"id":18},{"word":"to","speaker":"none","start":9.3,"end":9.4,"id":19},{"word":"give","speaker":"none","start":9.4,"end":9.5,"id":20},{"word":"you","speaker":"none","start":9.5,"end":9.6,"id":21},{"word":"holding","speaker":"none","start":9.6,"end":9.9,"id":22},{"word":"instructions.","speaker":"none","start":9.9,"end":10.5,"id":23}]
    }

    function highlightWord() {
        const currentTime = audioState.element.currentTime;

        // console.log(currentTime);
        // Get the current word (if there is one)
        const match = audioState.json.find(function (item) {
            return currentTime >= item.start && currentTime <= item.end;
        });

        if (match) {
            // Remove the .word-active class from the previously active word, which is now inactive
            const previousWord = document.querySelector(
            `[word-id="${audioState.activeWordId}"]`
            );
            // previousWord.classList.remove("word-active");

            // Add the .word-active class to the newly active word
            const currentWord = document.querySelector(`[word-id="${match.id}"]`);
            currentWord.classList.add("word-active");

            // Record the id for the current word
            audioState.activeWordId = match.id;

            // Show matching word in console
            // console.table(match);
        }
    }

    const playBtn = document.getElementById("audio-play-btn");
    const audio = document.getElementById("openerAudio");
    const icon = document.getElementById("audio-icon");
    let currentIconState = "play";

    icon.src = "https://static.propublica.org/projects/graphics/2025-spacex-flights/"+currentIconState+"_"+colorScheme+".svg";

    function updateIcon(iconState) {
        currentIconState = iconState;
        if (iconState === "play") {
            icon.src = "https://static.propublica.org/projects/graphics/2025-spacex-flights/play_"+colorScheme+".svg";
        } else if (iconState === "pause") {
            icon.src = "https://static.propublica.org/projects/graphics/2025-spacex-flights/pause_"+colorScheme+".svg";
        } else if (iconState === "replay") {
            icon.src = "https://static.propublica.org/projects/graphics/2025-spacex-flights/replay_"+colorScheme+".svg";
        }
    }

    function registerAudioEvents() {
        audioState.element.addEventListener("play", function (e) {
            // Run the highlightWord function every 100ms
            checkAudio = setInterval(highlightWord, 100);
            updateIcon("pause");
        });

        audioState.element.addEventListener("pause", function (e) {
            // Stop running the highlightWord function every 100ms
            clearInterval(checkAudio);
            updateIcon("play");
        });

        audioState.element.addEventListener("ended", function (e) {
            // Stop running the highlightWord function every 100ms
            clearInterval(audioState.interval);

            updateIcon("replay");
        });
    }

    function setupAudio() {
        audioState.element = audio;
        registerAudioEvents();
    }

    setupAudio();

    function removeActiveWords() {
        const allWords = document.querySelectorAll(".word");
        allWords.forEach((word) => {
            word.classList.remove("word-active");
        });
    }

    var transcript = document.getElementById("audioTranscript");

    playBtn.addEventListener("click", () => {
        transcript.style.display = "block";

        if (currentIconState === "replay") {
            removeActiveWords(); // ← Run when replay icon is clicked
            audio.currentTime = 0;
            audio.play();
            updateIcon("pause");
        }
        else if (audio.ended) {
            audio.currentTime = 0;
            audio.play();
            updateIcon("pause");
        } else if (audio.paused) {
            audio.play();
        } else {
            audio.pause();
        }
    });
});



</script>

</div>
</div>
</header>



<div class="wp-block-group article-body is-layout-flow wp-block-group-is-layout-flow">



<aside class="wp-block-group is-layout-flow wp-block-group-is-layout-flow"><div class="wp-block-propublica-notes--top wp-block-propublica-notes">
	<div class="wp-block-propublica-note">
	

<p>ProPublica is a nonprofit newsroom that investigates abuses of power. Sign up to receive <a href="https://www.propublica.org/newsletters/the-big-story?source=%25source%25">our biggest stories</a> as soon as they’re published.</p>

</div>
</div>
</aside>


<div class="entry-content wp-block-post-content is-layout-flow wp-block-post-content-is-layout-flow">




















<p>When SpaceX CEO Elon Musk chose a remote Texas outpost on the Gulf Coast to develop his company’s ambitious Starship, he put the 400-foot rocket on a collision course with the commercial airline industry.</p>



<p>Each time SpaceX did a test run of Starship and its booster, dubbed Super Heavy, the megarocket’s flight path would take it soaring over busy Caribbean airspace before it reached the relative safety of the open Atlantic Ocean. The company planned as many as five such launches a year as it perfected the craft, a version of which is supposed to one day land on the moon.</p>



<p>The FAA, which also oversees commercial space launches, predicted the impact to the national airspace would be “minor or minimal,” akin to a weather event, the agency’s 2022 approval shows. No airport would need to close and no airplane would be denied access for “an extended period of time.”&nbsp;</p>



<p>But the reality has been far different. Last year, three of Starship’s five launches exploded at unexpected points on their flight paths, twice raining flaming debris over congested commercial airways and disrupting flights. And while no aircraft collided with rocket parts, pilots were forced to scramble for safety.&nbsp;</p>



<p>A ProPublica investigation, based on agency documents, interviews with pilots and passengers, air traffic control recordings and photos and videos of the events, found that by authorizing SpaceX to test its experimental rocket over busy airspace, the FAA accepted the inherent risk that the rocket might put airplane passengers in danger.&nbsp;</p>



<p>And once the rocket failed spectacularly and that risk became real, neither the FAA nor Secretary of Transportation Sean Duffy sought to revoke or suspend Starship’s license to launch, a move that is permitted when “necessary to protect the public health and safety.” Instead, the FAA allowed SpaceX to test even more prototypes over the same airspace, adding stress to the already-taxed air traffic control system each time it launched.</p>



<p>The first two Starship explosions last year forced the FAA to make real-time calls on where to clear airspace and for how long. Such emergency closures came<strong> </strong>with little or no warning, ProPublica found, forcing pilots to suddenly upend their flight plans and change course in heavily trafficked airspace to get out of the way of falling debris. In one case, a plane with 283 people aboard ran low on fuel, prompting its pilot to declare an emergency and cross a designated debris zone to reach an airport.</p>



<p>The world’s largest pilots union told the FAA in October that such events call into question whether “a suitable process” is in place to respond to unexpected rocket mishaps.&nbsp;</p>



<p>“There is high potential for debris striking an aircraft resulting in devastating loss of the aircraft, flight crew, and passengers,” wrote Steve Jangelis, a pilot and aviation safety chair.</p>



<p>The FAA said in response to questions that it “limits the number of aircraft exposed to the hazards, making the likelihood of a catastrophic event extremely improbable.”&nbsp;</p>



<p>Yet for the public and the press, gauging that danger has been difficult. In fact, nearly a year after last January’s explosion, it remains unclear just how close Starship&#8217;s wreckage came to airplanes. SpaceX estimated where debris fell after each incident and reported that information to the federal government. But the company didn&#8217;t respond to ProPublica’s requests for that data, and the federal agencies that have seen it, including the FAA, haven’t released it. The agency told us that it was unaware of any other publicly available data on Starship debris.</p>



<p>In public remarks, Musk downplayed the risk posed by Starship. To caption a video of flaming debris in January, he wrote, “<a href="https://x.com/elonmusk/status/1880040599761596689?s=20">Entertainment is guaranteed!</a>” and, after the March explosion, he posted, “<a href="https://x.com/elonmusk/status/1897882353298506137?s=20">Rockets are hard</a>.” The company has been more measured, saying it learns from mistakes, which “help us improve Starship’s reliability.”&nbsp;</p>



<p>For airplanes traveling at high speeds, there is little margin for error. Research shows as little as 300 grams of debris — or two-thirds of a pound — “could catastrophically destroy an aircraft,” said Aaron Boley, a professor at the University of British Columbia who has studied the danger space objects pose to airplanes. Photographs of Starship pieces that washed up on beaches show items much bigger than that, <a href="https://www.accuweather.com/en/space-news/spacex-rocket-debris-litters-mexico-beach-threatens-environment/1782495">including large, intact tanks</a>.</p>



<figure class="wp-block-gallery has-nested-images columns-default is-cropped bb--size-large wp-block-gallery-4 is-layout-flex wp-block-gallery-is-layout-flex">
<figure class="wp-block-image size-large"><img fetchpriority="high" decoding="async" height="1024" width="768" data-id="60247" src="https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfile_true_quality_95.jpg?w=768" alt="Small brown, blue and white pieces of plastic scattered along a beach." class="wp-image-60247" srcset="https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfile_true_quality_95.jpg 1536w, https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfile_true_quality_95.jpg?resize=225,300 225w, https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfile_true_quality_95.jpg?resize=768,1024 768w, https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfile_true_quality_95.jpg?resize=1152,1536 1152w, https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfile_true_quality_95.jpg?resize=863,1151 863w, https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfile_true_quality_95.jpg?resize=422,563 422w, https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfile_true_quality_95.jpg?resize=552,736 552w, https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfile_true_quality_95.jpg?resize=558,744 558w, https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfile_true_quality_95.jpg?resize=527,703 527w, https://www.propublica.org/wp-content/uploads/2025/12/IMG_6172_preview_maxWidth_3000_maxHeight_3000_ppi_72_embedColorProfil