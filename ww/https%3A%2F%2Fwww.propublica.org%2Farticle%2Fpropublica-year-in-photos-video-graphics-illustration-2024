    <script src="https://static.propublica.org/projects/graphics/gsap/gsap.min.js"></script>

<script src="https://static.propublica.org/projects/graphics/gsap/ScrollTrigger.min.js"></script>

<script src="https://unpkg.com/split-type"></script>


<script>
  gsap.registerPlugin(ScrollTrigger);
  function splitTextIntoChars() {
                    const textToSplit = document.querySelectorAll(".scroller-text");
                    new SplitType(textToSplit, {types: "chars"});
                }
                function createScrollAnimation() {
                    const scrollerSection = document.querySelector(".scroller-section");
                    const chars = document.querySelectorAll(".char");
                    // Set characters to their initial fully visible state
                    gsap.set(chars, {
                        opacity: 1,
                        rotateY: 0,
                        yPercent: 0,
                        xPercent: 0,
                        scale: 1
                    });
                    const tl = gsap.timeline({
                        scrollTrigger: {
    trigger: scrollerSection,
    start: "center center",
    scrub: 1,
    pin: true,
     end: "+=160%",
                        }
                    });
                    // Animate characters to fade out and scatter
                    tl.to(chars, {
                        opacity: 0,
                        rotateY: () => Math.random() * 100 + 50,
                        yPercent: () => Math.random() * 100 - 50,
                        xPercent: () => Math.random() * 100 - 50,
                        stagger: {
                            amount: 1,
                            from: "random"
                        }
                    });
                }

                // Initialize when DOM is ready
                document.addEventListener("DOMContentLoaded", () => {
                    console.log(document.querySelector(".static-grid")); // Debugging
                    splitTextIntoChars();
                    createScrollAnimation();

                });
                const getY = (element) => {
                    const height = element.clientHeight;
                    const maxScrollSpeed = -400;
                    const minScrollSpeed = -10;
                    const referenceHeight = 500;
                    const speedFactor = maxScrollSpeed + (height / referenceHeight) * (minScrollSpeed - maxScrollSpeed);
                    console.log({height, speedFactor});
                    return speedFactor;
                };
                document.querySelectorAll(".grid_image").forEach((image) => {
                    gsap.to(image, {
                        y: getY(image),
                        ease: "none",
                        scrollTrigger: {
                            trigger: image,
                            start: "top bottom",
                            end: "bottom top",
                            scrub: 1
                        }
                    });
                });
// force a refresh on resize
window.addEventListener("resize", () => {
  ScrollTrigger.refresh();
});
  // horizontal scrollers
  
  const hScrollerList = document.querySelectorAll(".hs-scroller");
  const clampNumber = (min, num, max) =>
    Math.max(Math.min(num, Math.max(min, max)), Math.min(min, max));

  var clientHeight = 0;
  var clientWidth = 0;

  function hScrollerUpdateHeights() {
    hScrollerList.forEach((element) => {
      if (!clientHeight) {
        clientHeight = document.documentElement.clientHeight;
        clientWidth = document.documentElement.clientWidth;
      }
      element.setAttribute(
        "style",
        "height: " +
          element.querySelector(".hs-scroller__items").scrollWidth +
          "px"
      );
    });
  }

  function hScrollerUpdateScroll() {
    hScrollerList.forEach((element) => {
      const hScrollerBounds = element.getBoundingClientRect();

      const hScrollRatio =
        hScrollerBounds.top /
        (hScrollerBounds.height -
          clientHeight); /* Returns a number between -1 and 0, representing progress */
      const hScrollTotalAmount = element.querySelector(
        ".hs-scroller__items"
      ).scrollWidth;

      const translateAmount = Math.round(
        clampNumber(-1, hScrollRatio, 0) * (hScrollTotalAmount - clientWidth)
      );

      // if (hScrollerBounds.top <= 0 && hScrollerBounds.bottom > clientHeight) {
      element
        .querySelector(".hs-scroller__items")
        .setAttribute(
          "style",
          "transform: translateX(" + translateAmount + "px);"
        );
      // }
    });
  }

  ["DOMContentLoaded", "rafResize"].forEach(function (e) {
    window.addEventListener(e, function () {
      clientHeight = document.documentElement.clientHeight;
      clientWidth = document.documentElement.clientWidth;
      hScrollerUpdateHeights();
    });
  });

  ["rafScroll", "rafResize"].forEach(function (e) {
    window.addEventListener(e, function () {
      hScrollerUpdateScroll();
    });
  });
</script>
