    color:var(--gray-10);
    text-wrap:pretty;
   background-color:rgba(27,33,39,0.9);
}

    @media (max-width: 48em) {
        .custom-opener .step4-messages .message {
            margin: auto;
            max-width:100%;
            padding:15px;
        }
    }

.custom-opener .step4-messages .message .message-attribution {
    padding-top: 8px;
    font-weight: 400;
    font-size: var(--scale-2);
    color:var(--warm-20);
    color:var(--gray-10);

    font-family:var(--fonts-sans);
}

/* body styles */

.custom-opener.full {
    margin-top: 0;
}

.site-header--fixed {
    display: none;
}

</style>


