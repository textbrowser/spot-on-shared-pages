            openerMap.loadImage(
                "https://static.propublica.org/projects/graphics/2025-hurricane-helene/opener/highway-19E.png",
                (error, image) => {
                    if (error) throw error;

                    // 2. Add the image to the map style
                    openerMap.addImage("custom-icon", image);

                    // 3. Add the symbol layer using the custom image
                    openerMap.addLayer({
                        id: "highway-19E-symbol",
                        type: "symbol",
                        source: "highway-19E",
