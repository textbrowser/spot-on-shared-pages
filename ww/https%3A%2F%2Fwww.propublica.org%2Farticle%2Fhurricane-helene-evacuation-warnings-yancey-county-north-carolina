                color: var(--warm-20);
            }
            @media (prefers-color-scheme: dark) {
                html:not(.force-light-mode) #compass-overlay {
                    color: var(--warm-20);
                }
            }

            html.force-light-mode #compass-overlay {
                color: var(--gray-70);
            }
            @media (prefers-color-scheme: light) {
                html:not(.force-dark-mode) #compass-overlay {
                    color: var(--gray-70);
                }
            }


    #yancey-map #label__nc {
        font-size: 14px;
        font-weight: 600;
        letter-spacing: .1em;
        top: 22%;
        left: 79%;
        /* color: var(--gray-20); */
    }

            html.force-dark-mode #label__nc{
                color: var(--gray-10);
            }
            @media (prefers-color-scheme: dark) {
                html:not(.force-light-mode) #label__nc {
                    color: var(--gray-10);
                }
            }

            html.force-light-mode #label__nc {
                color: var(--gray-80);
            }
            @media (prefers-color-scheme: light) {
                html:not(.force-dark-mode) #label__nc {
                    color: var(--gray-80);
                }
            }



    @media (max-width: 48em) {
        #yancey-map .map-labels .home-location {
            font-size: 14px;
        }

        #yancey-map .map-labels #label__burnsville {
            top: 6%;
            font-size: 14px;
        }

        #yancey-map #label__strickland {
           top: 52%;
           right: 72%;
            bottom: 0;
        }

