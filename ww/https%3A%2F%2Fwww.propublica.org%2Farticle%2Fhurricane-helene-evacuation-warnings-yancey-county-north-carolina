<script>
document.addEventListener("DOMContentLoaded", function () {
    gsap.registerPlugin(ScrollTrigger)

     //set up screenSize
    let screenSize;

    function setScreenSize() {
        if (window.innerWidth < 749) {
            screenSize = "mobile";
        } else {
            screenSize = "desktop";
        }
    }

    setScreenSize();

    let resizeTimeout;

    window.addEventListener("resize", () => {

        if (screenSize === "desktop"){
            // Clear previous timeout to throttle execution
            clearTimeout(resizeTimeout);

            resizeTimeout = setTimeout(() => {
                ScrollTrigger.getAll().forEach((trigger) => {
                    const id = trigger.vars?.id;
                    if (id !== "animation-scroll") {
                        trigger.kill();
                    }
                });
                gsapTriggers();
                ScrollTrigger.refresh();
            }, 200);
        }

    });

        // SET UP SCROLLING ANIMATION

        let openerMap;
        let mapInitialized = false;
        let hasRunMapZoom1 = false;
        let layerId;
        let resizeCanvas;
        let targetFrame = 0;

        function setUpAnimation() {

            const frameCount = 59;
            let frameBasePath;

            if (screenSize === "desktop") {
                frameBasePath = "https://static.propublica.org/projects/graphics/2025-hurricane-helene/opener/desktop/desktop_";
            } else {
                frameBasePath = "https://static.propublica.org/projects/graphics/2025-hurricane-helene/opener/mobile/mobile_";
            }

            // Smooth lerping animation
            let currentFrame = 0;
            // let targetFrame = 0;

            const canvas = document.getElementById("frame-canvas");
            const ctx = canvas.getContext("2d");

            const images = [];
            const loaded = new Set();

            let drawWidth, drawHeight, offsetX, offsetY;
            let lastDrawnFrame = -1;

            // Pad number to 5 digits
            function pad(num) {
                return String(num).padStart(5, "0");
            }

            // Preload the first 10 frames eagerly
            for (let i = 0; i < 50; i++) {
                preloadFrame(i);
            }

            // Lazy-load others only as needed
            function preloadFrame(i) {
                if (images[i]) return;
                const img = new Image();
                img.src = `${frameBasePath}${pad(i)}.webp`;
                img.onload = () => {
                    loaded.add(i);
                    images[i] = img;
                    if (i === Math.round(currentFrame)) {
                        drawFrame(i); // Only draw if it's the current target frame
                    }
                };
            }

            // Resize canvas and calculate draw offset for cover-like behavior
            function resizeCanvas() {
                canvas.width = window.innerWidth;
                canvas.height = window.innerHeight;

                let imgWidth;
                let imgHeight;

                if (screenSize === "desktop") {
                    imgWidth = 1920;
                    imgHeight = 1080;
                } else {
                    imgWidth = 540;
                    imgHeight = 960;
                }

                const canvasRatio = canvas.width / canvas.height;
                const imgRatio = imgWidth / imgHeight;

                if (imgRatio > canvasRatio) {
                    drawHeight = canvas.height;
                    drawWidth = imgWidth * (canvas.height / imgHeight);
                    offsetX = (canvas.width - drawWidth) / 2;
                    offsetY = 0;
                } else {
                    drawWidth = canvas.width;
                    drawHeight = imgHeight * (canvas.width / imgWidth);
                    offsetX = 0;
                    offsetY = (canvas.height - drawHeight) / 2;
                }

                // Redraw current frame after resizing
                const rounded = Math.round(currentFrame);
                preloadFrame(rounded); // Make sure it's loaded
                drawFrame(rounded); // Redraw it
            }

            // window.addEventListener("resize", resizeCanvas);

            resizeCanvas();

            function drawFrame(index) {
                if (index === lastDrawnFrame) return;
                const img = images[index];
                if (!img || !loaded.has(index)) return;

                ctx.clearRect(0, 0, canvas.width, canvas.height);
                ctx.drawImage(img, offsetX, offsetY, drawWidth, drawHeight);
                lastDrawnFrame = index;
            }

             // Only one animate loop globally
            if (!window._animationStarted) {
                window._animationStarted = true;
                requestAnimationFrame(animate);
            }

            function animate() {
                currentFrame += (targetFrame - currentFrame) * 0.1;
                const rounded = Math.round(currentFrame);

                preloadFrame(rounded);
                if (loaded.has(rounded)) {
                    drawFrame(rounded);
                }
                requestAnimationFrame(animate);
            }


             // ScrollTrigger setup — only needs to be done once
            if (!window._scrollTriggerSetup) {
                window._scrollTriggerSetup = true;

                gsap.to(
                    {},
                    {
                        scrollTrigger: {
                            id: "animation-scroll",
                            trigger: "#step-1",
                            start: "top bottom",
                            end: "bottom 80%",
                            scrub: true,
                            onUpdate: (self) => {
                                targetFrame = self.progress * (frameCount - 1);
                            },
                        },
                    }
                );
            }
        }

        setUpAnimation();

        // set up resize of canvas when screen is resized
        let resizeDebounce;
        window.addEventListener("resize", () => {
            if (screenSize === "desktop") {
                clearTimeout(resizeDebounce);
                resizeDebounce = setTimeout(() => {
                    if (typeof resizeCanvas === "function") {
                        resizeCanvas();
                        ScrollTrigger.refresh();
                        console.log("Resized canvas without reloading images.");
                    }
                }, 200);
            }
        });

        // set up opener map so that it loads AFTER the scrolling animation loads

        function runMapInitOnce() {
                if (!mapInitialized) {
                    mapInitialized = true;
                    initializeOpenerMap();
                }
            }

        if (screenSize === "desktop") {
        /// set up step fades with intersectionObserver instead of gsap
        const fadeScroll = new IntersectionObserver(
            function (items, observer) {
                items.forEach((item) => {
                    if (item.isIntersecting) {
                        item.target.classList.add("fade-item--scrolled-into-view");
                    } else {
                        item.target.classList.remove("fade-item--scrolled-into-view");
                    }
                });
            },
            {
            root: null,
            rootMargin: "-20% 0px -20% 0px",
            threshold: 0.25,
            }
        );


        document.querySelectorAll(".fade-element").forEach((item) => fadeScroll.observe(item));
        }

        //// ALL GSAP CODE BELOW

        function gsapTriggers() {

            //////STEP 1

            let openerImageTL = gsap.timeline({
                scrollTrigger: {
                    id: "opener-image-trigger",
                    trigger: "#step-1",
                    start: "bottom bottom",
                    end: "bottom 30%",
                    scrub: true,
                    invalidateOnRefresh: true,
                // markers: false,
                }
            });

            openerImageTL.to("#opener-image-container",
                { opacity: 0,
                ease: "none"

                });

            // load map when you scroll into step-1
            ScrollTrigger.create({
                id: "init-map-trigger",
                trigger: "#step-1",
                start: "top 50%",
                once: true,
                onEnter: () => {
                    runMapInitOnce();

                },
            });

            // Fallback for if the page loads below the trigger
            window.addEventListener("load", () => {
                const triggerTop = document.querySelector("#step-1").getBoundingClientRect().top;
                const viewportHeight = window.innerHeight;

                if (triggerTop < viewportHeight * 0.5) {
                        runMapInitOnce();
                }
            });

            ScrollTrigger.create({
                id: "zoom0-trigger",
                trigger: "#step-1",
                start: "top top",
                onEnter: () => {
                    mapZoom0();
                },
            });

            /////// STEP 2

            ScrollTrigger.create({
                id: "zoom0-back-trigger",
                trigger: "#step-1",
                start: "bottom bottom",
                onEnterBack: () => {
                    mapZoom0();
                    fadeOutDotsLayer();
                },
                markers: false,

            });

            ///// STEP 3

            ScrollTrigger.create({
                id: "fadeindots-trigger",
                trigger: "#step-3",
                start: "top 80%",
                onEnter: () => {
                    mapZoom1();
                    if (openerMap.isStyleLoaded()) {
                        fadeInDotsLayer();
                    }
                },
                invalidateOnRefresh: true,

            });

            //////// STEP 4

            ScrollTrigger.create({
                id: "zoom1-trigger",
                trigger: "#step-4",
                start: "top 80%",
                onEnter: () => {
                    if (!hasRunMapZoom1) {
                        mapZoom1();
                    }
                },
                invalidateOnRefresh: true,
            });

            ScrollTrigger.create({
                id: "opener-image-trigger",
                trigger: "#step-3",
                start: "bottom 50%",
                onEnterBack: () => {
                    runMapInitOnce();
                    if (openerMap.isStyleLoaded()) {
                        fadeInDotsLayer();
                    }
                },
            });

            ////// Step 5

            // Fade out map
            const fadeOutMapTimeline = gsap.timeline();

            fadeOutMapTimeline.fromTo("#opener-map",
                { opacity: 1 },
                { opacity: 0, ease: "none" }
            );

            ScrollTrigger.create({
                id: "fadeoutmap-trigger",
                trigger: "#step-5",
                start: "top bottom",
                end: "bottom bottom", // adjust for how much scroll range controls the fade
                scrub: true,
                animation: fadeOutMapTimeline,
                // markers: true,
            });

            ScrollTrigger.create({
                id: "zoom2-trigger",
                trigger: "#step-5",
                start: "top bottom",
                onEnterBack: () => {
                    mapZoom2();

                },
                // invalidateOnRefresh: true,
            });

            ScrollTrigger.create({
                id: "flicker-trigger",
                trigger: "#step-4",
                start: "top 20%",
                onEnter: () => {
                    if (openerMap.isStyleLoaded()) {
                        startFlickerEffect()
                    }
                },
                // markers: true,
            });

             ScrollTrigger.refresh();

        }

        gsapTriggers();

        window.addEventListener("load", () => {
            window.scrollTo(window.scrollX, window.scrollY + 1);
            window.scrollTo(window.scrollX, window.scrollY - 1);
            ScrollTrigger.refresh();
         });


        ////// MAP SET UP

        function initializeOpenerMap() {

            mapboxgl.accessToken =
                "pk.eyJ1IjoicHJvcHVibGljYSIsImEiOiJjaWt6dzNnNjQxbHBudTlrc3Nqc3NidG5qIn0.ChQFj8IDuUNxSg_I6_3Pnw";

            let centerPoint, zoomAmount;

            if (screenSize === "desktop") {
                centerPoint = [-82.29276, 35.879983];
                zoomAmount = 12.815;
            } else {
                centerPoint = [-82.31, 35.879983]; // You can change this for mobile
                zoomAmount = 12.1; // Example: zoomed out a bit more for mobile
            }

            openerMap = new mapboxgl.Map({
                container: "opener-map",
                style: "mapbox://styles/propublica/cm88yi9y500ex01rgho767gp4",
                center: centerPoint,
                zoom: zoomAmount,
                pitch: 65.5, // Tilting
                bearing: -180, // Rotating
                antialias: true, // Smoother edges
                interactive: false,
            });

            window.openerMap = openerMap;


            openerMap.on("load", () => {

                // Add the DEM source for terrain
                openerMap.addSource("mapbox-dem", {
                    type: "raster-dem",
                    url: "mapbox://mapbox.terrain-rgb",
                    tileSize: 512,
                    maxzoom: 14,
                });

                // Apply terrain after the source is added
                openerMap.setTerrain({ source: "mapbox-dem", exaggeration: 1.5 });

                // Optional: Add a hillshade layer for better visualization
                openerMap.addLayer({
                    id: "hillshade",
                    source: "mapbox-dem",
                    type: "hillshade",
                });

                layerId = 'homes-ids-1ag917';

                // openerMap.getStyle().layers.forEach(layer => console.log(layer.id));

                addOpenerMapLayers(openerMap);
            });
        }

        /////// HANDLE DOTS

        function fadeInDotsLayer(duration = 500) {

            openerMap.setPaintProperty(layerId, 'circle-opacity', 0);
            openerMap.setPaintProperty(layerId, 'circle-opacity-transition', {
                duration: duration,
                delay: 0,
            });
            openerMap.setPaintProperty(layerId, 'circle-opacity', .7);

            openerMap.setPaintProperty(layerId, 'circle-stroke-opacity', 0);
            openerMap.setPaintProperty(layerId, 'circle-stroke-opacity-transition', {
                duration: duration,
                delay: 0,
            });
            openerMap.setPaintProperty(layerId, 'circle-stroke-opacity', 1);

            // prepare flicker
            setTimeout(() => {
                 prepareFeatureState();
            }, duration + 100);
        }

        function fadeOutDotsLayer(duration = 500) {

            openerMap.setPaintProperty(layerId, 'circle-opacity', .7);
            openerMap.setPaintProperty(layerId, 'circle-opacity-transition', {
                duration: duration,
                delay: 0,
            });
            openerMap.setPaintProperty(layerId, 'circle-opacity', 0);

            openerMap.setPaintProperty(layerId, 'circle-stroke-opacity', 1);
            openerMap.setPaintProperty(layerId, 'circle-stroke-opacity-transition', {
                duration: duration,
                delay: 0,
            });
            openerMap.setPaintProperty(layerId, 'circle-stroke-opacity', 0);
        }

        function prepareFeatureState() {
            const sourceId = openerMap.getLayer(layerId).source;
            const sourceLayer = openerMap.getLayer(layerId).sourceLayer;

            const features = openerMap.querySourceFeatures(sourceId, {
                sourceLayer: sourceLayer,
            });

            for (const feature of features) {
                const featureId = feature.id;
                if (featureId === undefined) continue;

                openerMap.setFeatureState(
                    { source: sourceId, sourceLayer, id: featureId },
                    {
                        circleOpacity: 0.7,
                        strokeOpacity: 1
                    }
                );
            }

            // Defer paint property switch to next frame
            requestAnimationFrame(() => {
                openerMap.setPaintProperty(layerId, 'circle-opacity', [
                    'coalesce',
                    ['feature-state', 'circleOpacity'],
                    0.7
                ]);

                openerMap.setPaintProperty(layerId, 'circle-stroke-opacity', [
                    'coalesce',
                    ['feature-state', 'strokeOpacity'],
                    1
                ]);
            });
        }

        function startFlickerEffect() {
            const sourceId = openerMap.getLayer(layerId).source;
            const sourceLayer = openerMap.getLayer(layerId).sourceLayer;

            const features = openerMap.querySourceFeatures(sourceId, {
                sourceLayer: sourceLayer,
            });

            const flickeredThisCycle = new Set();

            for (const feature of features) {
                const featureId = feature.id;
                if (featureId === undefined || flickeredThisCycle.has(featureId)) continue;

                flickeredThisCycle.add(featureId);

                const delay = Math.random() * 4000;

                setTimeout(() => {
                    const sequence = [
                        { opacity: 0.4, duration: 200 },
                        { opacity: 0.8, duration: 200 },
                        { opacity: 0, duration: 200 },
                    ];

                    let step = 0;

                    function animateStep() {
                        if (step >= sequence.length) return;

                        const { opacity, duration } = sequence[step];

                        openerMap.setFeatureState(
                            { source: sourceId, sourceLayer, id: featureId },
                            {
                                circleOpacity: opacity,
                                strokeOpacity: opacity
                            }
                        );

                        step++;
                        setTimeout(animateStep, duration);
                    }

                    animateStep();
                }, delay);
            }
        }

        /////// MAP ZOOM LEVELS

        function mapZoom0() {
            if (!openerMap || !openerMap.isStyleLoaded()) return;
            if (screenSize === "desktop") {
                openerMap.easeTo({
                    center: [-82.29276, 35.879983],
                    zoom: 12.815,
                    pitch: 65.5, // Tilting
                    bearing: -180, // Rotating
                    duration: 1500,
                });
            } else {
                openerMap.easeTo({
                    center: [-82.31, 35.879983],
                    zoom: 12.1,
                    pitch: 65.5, // Tilting
                    bearing: -180, // Rotating
                    duration: 1500,
                });
            }

            document.getElementById('compass-overlay').style.transform = "rotate(180deg)"
        }

        function mapZoom1() {
            if (!openerMap || !openerMap.isStyleLoaded()) return;

            if (screenSize === "desktop") {
                openerMap.easeTo({
                    center: [-82.302698, 35.84338],
                    zoom: 14.52,
                    pitch: 66.5, // Tilting
                    bearing: -190, // Rotating
                    duration: 1500,
                });
            } else {
                openerMap.easeTo({
                    center: [-82.302698, 35.84338],
                    zoom: 13.8,
                    pitch: 66.5, // Tilting
                    bearing: -210, // Rotating
                    duration: 1500,
                });
            }

            openerMap.once("moveend", () => {
                // drawHomeDots();
            });

            if (screenSize === "desktop") {
                document.getElementById('compass-overlay').style.transform = "rotate(190deg)"
            } else {
                document.getElementById('compass-overlay').style.transform = "rotate(210deg)"
            }

            hasRunMapZoom1 = true;


        }

        function mapZoom2() {
            if (!openerMap || !openerMap.isStyleLoaded()) return;

            console.log("mapZoom2 is running")
            if (screenSize === "desktop") {
                openerMap.easeTo({
                    center: [-82.302698, 35.84338],
                    zoom: 14.52,
                    pitch: 66.5, // Tilting
                    bearing: -190, // Rotating
                });
            } else {
                openerMap.easeTo({
                    center: [-82.302698, 35.84338],
                    zoom: 13.8,
                    pitch: 66.5, // Tilting
                    bearing: -210, // Rotating
                });
            }
        }

