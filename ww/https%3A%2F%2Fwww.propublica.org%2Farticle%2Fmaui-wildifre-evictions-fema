                            data-src="https://assets-c3.propublica.org/images/getInvolved/_oneOne75w/230810-kf-Maui-Fire-129-1x1_maxWidth_3000_maxHeight_3000_ppi_72_quality_95_embedColorProfile_true.jpg"
                            data-srcset="https://assets-c3.propublica.org/images/getInvolved/_oneOne75w/230810-kf-Maui-Fire-129-1x1_maxWidth_3000_maxHeight_3000_ppi_72_quality_95_embedColorProfile_true.jpg 75w, https://assets-c3.propublica.org/images/getInvolved/_oneOne150w/230810-kf-Maui-Fire-129-1x1_maxWidth_3000_maxHeight_3000_ppi_72_quality_95_embedColorProfile_true.jpg 150w"
