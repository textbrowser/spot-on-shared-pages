<script>

document.addEventListener("DOMContentLoaded", function() {

    const iufdData = [
        {
            "year": 2017,
            "other_admissions_pct": 0.03732638889,
            "no_heartbeat_pct": 0.020657277
        },
        {
            "year": 2018,
            "other_admissions_pct": 0.04286964129,
            "no_heartbeat_pct": 0.01536885246
        },
        {
            "year": 2019,
            "other_admissions_pct": 0.04735376045,
            "no_heartbeat_pct": 0.02290836653
        },
        {
            "year": 2020,
            "other_admissions_pct": 0.03972868217,
            "no_heartbeat_pct": 0.02272727273
        },
        {
            "year": 2021,
            "other_admissions_pct": 0.04460966543,
            "no_heartbeat_pct": 0.01559934319
        },
        {
            "year": 2022,
            "other_admissions_pct": 0.05595854922,
            "no_heartbeat_pct": 0.0297029703
        },
        {
            "year": 2023,
            "other_admissions_pct": 0.06948968512,
            "no_heartbeat_pct": 0.03139013453
        }
    ];

    function drawIUFDChart() {

            const margin = { top: 20, right: 20, bottom: 30, left: 20 };
            const iufdContainer = d3.select(".iufd-chart-container");
            const iufdsvg = iufdContainer.select("#iufdChartSvg");

            const width = iufdContainer.node().clientWidth;
            const height = width * (window.innerWidth > 1000 ? 2 / 3 : 1);

            iufdsvg.attr("viewBox", `0 0 ${width} ${height}`);

            const innerWidth = width - margin.left - margin.right;
            const innerHeight = height - margin.top - margin.bottom;

            const xScale = d3.scaleLinear()
                .domain([2017, 2023])
                .range([0, innerWidth]);

            const yScale = d3.scaleLinear()
                .domain([0, .075]) // Adjusts to data range
                .range([innerHeight, 0]);

            const g = iufdsvg.append("g")
                .attr("transform", `translate(${margin.left},${margin.top})`);

            const xAxis = d3.axisBottom(xScale)
                .tickValues([2017,2019,2021,2023])
                .tickFormat(d3.format(".0f"))
                .ticks(iufdData.length);

            const yAxis = d3.axisLeft(yScale)
                .tickValues([ ])
                .tickFormat(d => `${d}%`);

            g.append("g")
                .attr("transform", `translate(0,${innerHeight})`)
                .attr("class", "axis")
                .call(xAxis);

            g.append("g")
                .attr("class", "axis")
                .call(yAxis);


            // rect for ban years
            g.append("rect")
                .attr("id", "rectIUFDBanYears")
                .attr("x", xScale(2021))
                .attr("width", xScale(2023) - xScale(2021))
                .attr("y", 0)
                .attr("height", innerHeight)
                .style("opacity", .5)


            const otherLine = d3.line()
                .x(d => xScale(d.year))
                .y(d => yScale(d.other_admissions_pct));

            const noHeartbeatLine = d3.line()
                .x(d => xScale(d.year))
                .y(d => yScale(d.no_heartbeat_pct));

            g.append("path")
                .datum(iufdData)
                .attr("fill", "none")
                .attr("stroke", "#a0442e")
                .attr("stroke-width", 2)
                .attr("d", noHeartbeatLine);

            g.append("path")
                .datum(iufdData) // Bind data
                .attr("fill", "none")
                .attr("stroke", "#d29d05")
                .attr("stroke-width", 2)
                .attr("d", otherLine);

            //////// CIRCLE: OTHER LINE FIRST POINT
            const otherFirstPoint = iufdData.find(d => d.year === 2017);

            // Draw circle on first data point
            const otherFirstX = xScale(otherFirstPoint.year);
            const otherFirstY = yScale(otherFirstPoint.other_admissions_pct);

            // Append a circle at the start of the line
            g.append("circle")
                .attr("cx", otherFirstX)
                .attr("cy", otherFirstY)
                .attr("opacity", 1)
                .attr("class","other-circle")
                .attr("id","other-first__circle")
                .attr("r", 5);// 5px diameter

            ///////// CIRCLE: OTHER LINE SECOND POINT
            const otherSecondPoint = iufdData.find(d => d.year === 2023);

            // Draw circle on first data point
            const otherSecondX = xScale(otherSecondPoint.year);
            const otherSecondY = yScale(otherSecondPoint.other_admissions_pct);

            // Append a circle at the start of the line
            g.append("circle")
                .attr("cx", otherSecondX)
                .attr("cy", otherSecondY)
                .attr("opacity", 1)
                .attr("class","other-circle")
                .attr("id","other-second__circle")
                .attr("r", 5);// 5px diameter


            //////// CIRCLE: NOHEARTBEAT LAST  POINT

            const noheartbeatLastPoint = iufdData.find(d => d.year === 2023);

            // Draw circle on first data point
            const noheartbeatLastPointX = xScale(noheartbeatLastPoint.year);
            const noheartbeatLastPointY = yScale(noheartbeatLastPoint.no_heartbeat_pct);

            // Append a circle at the start of the line
            g.append("circle")
                .attr("cx", noheartbeatLastPointX)
                .attr("cy", noheartbeatLastPointY)
                .attr("opacity", 1)
                .attr("class","noheartbeat-circle")
                .attr("id","noheartbeat-Last__circle")
                .attr("r", 5);// 5px diameter

            //////// CIRCLE: NOHEARTBEAT FIRST POINT

            const noheartbeatFirstPoint = iufdData.find(d => d.year === 2017);

            // Draw circle on first data point
            const noheartbeatFirstPointX = xScale(noheartbeatFirstPoint.year);
            const noheartbeatFirstPointY = yScale(noheartbeatFirstPoint.no_heartbeat_pct);

            // Append a circle at the start of the line
            g.append("circle")
                .attr("cx", noheartbeatFirstPointX)
                .attr("cy", noheartbeatFirstPointY)
                .attr("opacity", 1)
                .attr("class","noheartbeat-circle")
                .attr("id","noheartbeat-first__circle")
                .attr("r", 5);// 5px diameter

        // close drawIUFDChart below
    }

    drawIUFDChart();

    function resizeIUFDChart() {
        // Remove the existing chart content
        d3.select("#iufdChartSvg").selectAll("*").remove();

        // Redraw the chart
        drawIUFDChart();

    }

    window.addEventListener("resize", resizeIUFDChart);


    // download SVG

    // document.getElementById("downloadSvg").addEventListener("click", function () {
    //         const chartsvg = document.getElementById("iufdChartSvg");
    //         const serializer = new XMLSerializer();
    //         let source = serializer.serializeToString(chartsvg);

    //         // Add the XML namespace if missing
    //         if (!source.includes("xmlns")) {
    //             source = source.replace("<svg", '<svg xmlns="http://www.w3.org/2000/svg"');
    //         }

    //         // Create a Blob and a download link
    //         const blob = new Blob([source], { type: "image/svg+xml" });
    //         const url = URL.createObjectURL(blob);
    //         const a = document.createElement("a");
    //         a.href = url;
    //         a.download = "downloaded-svg.svg";
    //         document.body.appendChild(a);
    //         a.click();
    //         document.body.removeChild(a);
    //         URL.revokeObjectURL(url);
    //     });

    })



</script>
            
    
<figcaption class="attribution">
        <span class="attribution__caption">Note: For hospitalizations involving a pregnancy loss between 13 weeks’ gestation and the end of the 21st week. We identified patients whose fetus had no heartbeat when they were admitted by looking for a diagnosis of “intrauterine death” or “missed abortion.” Rates are annual.</span>
    
