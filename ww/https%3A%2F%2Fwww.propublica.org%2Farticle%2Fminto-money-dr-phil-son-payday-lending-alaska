        <script>
        const noAds = new URLSearchParams(window.location.search).get('__no-ads');
        let adTimeout = new URLSearchParams(window.location.search).get('__ad-timeout');
        if( noAds === null ) {
            if( adTimeout === null ) { adTimeout = 0; }
            setTimeout(function() {
                PP.utils.loadJS('https://htlbid.com/v3/propublica.org/htlbid.js',function() {
                    window.htlbid = window.htlbid || {};
                    htlbid.cmd = htlbid.cmd || [];
                    htlbid.cmd.push(function() {
                        htlbid.layout("universal"); // Leave as "universal" or add custom layout
                                                    htlbid.setTargeting("is_testing", "no");
                                                htlbid.setTargeting("is_home", "no"); // Set to "yes" on the homepage
                        htlbid.setTargeting("post_tags", "debt");
                    });
                });
            }, adTimeout);
        }
    </script>

