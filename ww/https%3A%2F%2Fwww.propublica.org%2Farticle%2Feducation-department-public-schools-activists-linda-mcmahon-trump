<script>
  document.addEventListener("DOMContentLoaded", function () {

    const visuals = document.querySelectorAll(".custom-opener--item");
    const textBoxes = document.querySelectorAll(".custom-opener--text-box");

    function initObserver(threshold) {
      const observer = new IntersectionObserver((entries) => {
        entries.forEach(entry => {
          const index = [...textBoxes].indexOf(entry.target);
          if (index !== -1) {
            if (entry.isIntersecting) {
              visuals.forEach(v => v.classList.remove("active"));
              visuals[index].classList.add("active");
              console.log(index);
            }
          }
        });
      }, {
        root: null,
        threshold: threshold
      });

      textBoxes.forEach(text => observer.observe(text));
    }

    const mm = window.matchMedia("(max-width:60em)");
    if (mm.matches) {
      initObserver(0.5);
      console.log("mobile media matches");
    } else {
      initObserver(0.5);
      console.log("desktop media matches");
    }
    mm.addEventListener("change", (e) => {
      location.reload();
    });
  });



</script>
