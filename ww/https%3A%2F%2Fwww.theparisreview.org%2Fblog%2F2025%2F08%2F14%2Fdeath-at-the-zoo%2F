							if (!data.error) {
								$('.modal-state:checked').prop('checked', false).change();
								$('.sign-in-name').html('Welcome, '+data.name+' <a href="/authentication/sign_out" ><small>(Sign out)</small></a>');
								window.dataLayer = window.dataLayer || [];
								window.dataLayer.push({
									'event': 'login',
									'userId': data.ga_user_id
								});
								$("#signin-form-button").prop('disabled', false);
									if (typeof check_article_auth == 'function') {
										check_article_auth();
									}
							} else {
								$("#signin-form-button").prop('disabled', false);
								$('#signin_error').text(data.error);
							}
						},
							error: function(data) {
								$("#signin-form-button").prop('disabled', false);
								var response = jQuery.parseJSON( data.responseText );
								$('#signin_error').text("There was an error signing in. Please try again.");
							},
					});
					event.preventDefault();
				});

				$("#signin-form-mobile").submit(function(event) {
					$("#signin-form-button-mobile").prop('disabled', true);
					var url = "/authentication";
					$.ajax({
						type: "POST",
						url: url,
						data: $("#signin-form-mobile").serialize(),
						success: function(data) {
							if (!data.error) {
								$('.modal-state:checked').prop('checked', false).change();
								$('.sign-in-name').html('Welcome, '+data.name+' <a href="/authentication/sign_out" ><small>(Sign out)</small></a>');
								window.dataLayer = window.dataLayer || [];
								window.dataLayer.push({
									'event': 'login',
									'userId': data.ga_user_id
								});
								$("#signin-form-button-mobile").prop('disabled', false);
									if (typeof check_article_auth == 'function') {
										check_article_auth();
									}
							} else {
								$("#signin-form-button-mobile").prop('disabled', false);
								$('#signin_error').text(data.error);
							}
						},
							error: function(data) {
								$("#signin-form-button-mobile").prop('disabled', false);
								var response = jQuery.parseJSON( data.responseText );
								$('#signin_error-mobile').text("There was an error signing in. Please try again.");
							},
					});
					event.preventDefault();
				});

				var name_url = "/authentication/get-name";
				$.ajax({
					type: "GET",
					url: name_url,
					success: function(data) {
						if (!data.error) {
							$('.sign-in-name').html('Welcome, '+data.name+' <a href="/authentication/sign_out" ><small>(Sign out)</small></a>');
							window.dataLayer = window.dataLayer || [];
							window.dataLayer.push({
								'event': 'verified_login',
								'userId': data.ga_user_id
							});
						}
					},
				});
			});

			$('a.icon-print').click(function(){
				window.print();
				return false;
			});

			$('a.icon-email').click(function(){
				event.preventDefault();
				var url      = window.location.href; 
				var subject = 'From the Paris Review '+article_title;
				var emailBody = article_title+" \n\n "+url;
				window.location = 'mailto:?subject=' + subject + '&body=' +   emailBody;
				return false;
			});

			$('a.icon-facebook').click(function(){
				event.preventDefault();
				var url      = window.location.href;
				window.open('https://www.facebook.com/share.php?u=' + url, '_blank');
				return false;
			});

			$('a.icon-twitter').click(function(){
				event.preventDefault();
				var url      = window.location.href;
				window.open('https://twitter.com/share?text='+article_title+'&url=' + url, '_blank');
				return false;
			});
		</script>

      <script>
        (function (w,i,d,g,e,t,s) {w[d] = w[d]||[];t= i.createElement(g);
          t.async=1;t.src=e;s=i.getElementsByTagName(g)[0];s.parentNode.insertBefore(t, s);
        })(window, document, '_gscq','script','//widgets.getsitecontrol.com/69508/script.js');
      </script>
	</body>
</html>