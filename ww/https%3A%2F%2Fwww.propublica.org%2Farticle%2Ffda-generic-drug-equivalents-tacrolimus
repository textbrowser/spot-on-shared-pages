<script src="https://propublica.s3.amazonaws.com/projects/graphics/gsap/gsap.min.js"></script>
<script src="https://propublica.s3.amazonaws.com/projects/graphics/gsap/ScrollTrigger.min.js"></script>
<script>
    gsap.registerPlugin(ScrollTrigger);

    const defaultBg = getComputedStyle(document.body).backgroundColor;
    let activeSections = 0;

    gsap.utils
        .toArray(".hannah-supplied-container")
        .forEach((container) => {

            const images = container.querySelectorAll(".hannah-image");
            const caption = container.querySelector(".hannah-caption");

            let bgColor;

            /* colors and dark mode */
            const media = window.matchMedia("(prefers-color-scheme: dark)");
            let darkMode =
                media.matches ||
                document.documentElement.classList.contains("force-dark-mode");

            if (media.matches) {
                if (document.documentElement.classList.contains("force-light-mode")) {
                    darkMode = false;
                }
            } else if (document.documentElement.classList.contains("force-dark-mode")) {
                darkMode = true;
            }

            function gsapColors() {
                bgColor = darkMode ? "#5A4D6D" : "#DACDE0";
            }

            gsapColors();

        



            /* bg */
            ScrollTrigger.create({
                trigger: container,
                start: "top center",
                end: "bottom center",
                // markers: true,

                onToggle: self => {
                    if (self.isActive) {
                        activeSections++;

                        gsap.to("body", {
                            backgroundColor: bgColor,
                            duration: 0.8,
                            ease: "power1.out",
                            overwrite: "auto"
                        });

                        gsap.to("header .site-header--fixed", {
                        backgroundColor: bgColor,
                        duration: 0.8,
                        ease: "power1.out",
                        overwrite: "auto"
                       });

                    } else {
                        activeSections--;


                        if (activeSections === 0) {
                            gsap.to("body", {
                                backgroundColor: defaultBg,
                                duration: 0.8,
                                ease: "power1.out",
                                overwrite: "auto"
                            });

                               gsap.to("header .site-header--fixed", {
                                    backgroundColor: defaultBg,
                                    duration: 0.8,
                                    ease: "power1.out",
                                    overwrite: "auto"
                                });
                                    }
                    }
                }
            });

            /* images */
            if (images.length) {
                gsap.to(images, {
                    opacity: 1,
                    stagger: 0.3,
                    scrollTrigger: {
                        trigger: container,
                        start: "top 40%",
                        end: "+=400",
                        scrub: 1,
                        immediateRender: false,
                        invalidateOnRefresh: true
                    }
                });
            }

            /* caption */
            if (caption) {
                gsap.to(caption, {
                    opacity: 1,
                    scrollTrigger: {
                        trigger: container,
                        start: "top top",
                        end: "+=300",
                        scrub: 1,
                        immediateRender: false,
                        invalidateOnRefresh: true
                    }
                });
            }
        });


</script><script id="wp-block-template-skip-link-js-after">
	( function() {
		var skipLinkTarget = document.querySelector( 'main' ),
			sibling,
			skipLinkTargetID,
			skipLink;

		// Early exit if a skip-link target can't be located.
		if ( ! skipLinkTarget ) {
			return;
		}

		/*
		 * Get the site wrapper.
		 * The skip-link will be injected in the beginning of it.
		 */
		sibling = document.querySelector( '.wp-site-blocks' );

		// Early exit if the root element was not found.
		if ( ! sibling ) {
			return;
		}

		// Get the skip-link target's ID, and generate one if it doesn't exist.
		skipLinkTargetID = skipLinkTarget.id;
		if ( ! skipLinkTargetID ) {
			skipLinkTargetID = 'wp--skip-link--target';
			skipLinkTarget.id = skipLinkTargetID;
		}

		// Create the skip link.
		skipLink = document.createElement( 'a' );
		skipLink.classList.add( 'skip-link', 'screen-reader-text' );
		skipLink.id = 'wp-skip-link';
		skipLink.href = '#' + skipLinkTargetID;
		skipLink.innerText = 'Skip to content';

		// Inject the skip link.
		sibling.parentElement.insertBefore( skipLink, sibling );
	}() );
	
</script>
