<script type="text/javascript">

if (!$.cookie('hide_sitenotice_36')) {
	$('#announcement').fadeIn(300);
}

function closeAnnouncement() {
	$('#announcement').fadeOut(300);
	$.cookie('hide_sitenotice_36', 'true', { path: '/', domain: 'slashdot.org', expires: 1 });
}
</script>
				<article id="firehose-174545694" data-fhid="174545694" data-fhtype="story" class="fhitem fhitem-story article usermode thumbs grid_24">
		<span class="sd-info-block" style="display: none">
			<span class="sd-key-firehose-id">174545694</span>
			<span class="type">story</span>
			
		</span>







	
	

<header>
	
		<span class="topic" id="topic-174545694">
			<a href="//news.slashdot.org/index2.pl?fhfilter=firefox" onclick="return addfhfilter('firefox');">
			
				<img src="//a.fsdn.com/sd/topics/firefox_64.png" width="64" height="64" alt="Firefox" title="Firefox">
			
		</a>
		
	
		
		
		
		

		
		<a href="//news.slashdot.org/index2.pl?fhfilter=advertising" onclick="return addfhfilter('advertising');">
			
