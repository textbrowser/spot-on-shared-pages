	s.setAttribute('onerror', "!async function(){let t=JSON.parse(atob('WyJodG1sLWxvYWQuY29tIiwiZmIuaHRtbC1sb2FkLmNvbSIsImNvbnRlbnQtbG9hZGVyLmNvbSIsImZiLmNvbnRlbnQtbG9hZGVyLmNvbSJd'));const e=window,r=t=>{const r=navigator.language.toLowerCase().startsWith('ko')?decodeURIComponent(escape(atob('7J6Y66q765CcIOyVoOuTnOu4lOuhnSDtlYTthLDroZwg7Ju57Y6Y7J207KeAIOuhnOuUqeyXkCDsi6TtjKjtlZjsmIDsirXri4jri6QuIOyVoOuTnOu4lOuhneydhCDruYTtmZzshLHtmZTtlZwg7ZuEIOybueyCrOydtO2KuOulvCDsnbTsmqntlbTso7zshLjsmpQuICjsnpDshLjtlZwg64K07Jqp7J2AICftmZXsnbgnIOuyhO2KvOydhCDriIzrn6wg7ZmV7J247ZW07KO87IS47JqUKQ'))):atob('VGhpcyBwYWdlIGNvdWxkIG5vdCBiZSBsb2FkZWQgcHJvcGVybHkgZHVlIHRvIGluY29ycmVjdCAvIGJhZCBmaWx0ZXJpbmcgcnVsZShzKSBvZiBhZGJsb2NrZXJzIGluIHVzZS4gUGxlYXNlIGRpc2FibGUgYWxsIGFkYmxvY2tlcnMgdG8gY29udGludWUgdXNpbmcgdGhlIHdlYnNpdGUuIChjbGljayBPSyBpZiB5b3UnZCBsaWtlIHRvIGxlYXJuIG1vcmUp');if(confirm(r)){const r=new e.URL('https://report.error-report.com/modal');r.searchParams.set('url',btoa(location.href)),r.searchParams.set('error',btoa(t.toString())),r.searchParams.set('domain',domain),location.href=r.href}else location.reload()};try{if(void 0===window.as_retry&&(window.as_retry=0),window.as_retry>=t.length)throw atob('RmFpbGVkIHRvIGxvYWQgd2Vic2l0ZSBwcm9wZXJseSBzaW5jZSA')+t[0]+atob('IGlzIGJsb2NrZWQuIFBsZWFzZSBhbGxvdyA')+t[0];const r=document.getElementById('vPeSmTdWXS'),o=document.createElement('script');for(let t=0;t<r.attributes.length;t++)o.setAttribute(r.attributes[t].name,r.attributes[t].value);const a=new e.URL(r.getAttribute('src'));a.host=t[window.as_retry++],o.setAttribute('src',a.href),r.setAttribute('id',r.getAttribute('id')+'_'),r.parentNode.insertBefore(o,r),r.remove()}catch(t){console.error(t);try{window.localStorage.setItem(window.location.host+'_fa_'+btoa('last_bfa_at'),Date.now().toString())}catch(t){}try{setInterval((()=>document.querySelectorAll('link,style').forEach((t=>t.remove()))),100),alert(t);const o=await(await fetch('https://error-report.com/report?type=loader_light&url='+btoa(location.href)+'&error='+btoa(t),{method:'POST'})).text();let a=!1;window.addEventListener('message',(t=>{'as_modal_loaded'===t.data&&(a=!0)}));let s=document.createElement('iframe');const n=new e.URL('https://report.error-report.com/modal');n.searchParams.set('url',btoa(location.href)),n.searchParams.set('eventId',o),n.searchParams.set('error',btoa(t)),n.searchParams.set('domain',domain),s.src=n.href,s.setAttribute('style','width: 100vw; height: 100vh; z-index: 2147483647; position: fixed; left: 0; top: 0;');const i=t=>{'close-error-report'===t.data&&(s.remove(),window.removeEventListener('message',i))};window.addEventListener('message',i),document.body.appendChild(s);const l=()=>{const t=s.getBoundingClientRect();return'none'!==e.getComputedStyle(s).display&&0!==t.width&&0!==t.height},d=setInterval((()=>{if(!document.contains(s))return clearInterval(d);l()||(r(t),clearInterval(d))}),1e3);(()=>{const t=window.navigator.userAgent;return!['chrome','edge','edg/','opr','opera','opt/','trident','crios','whale','ddg/','naver','edgios'].some((e=>t.includes(e)))&&['safari','firefox'].some((e=>t.includes(e)))})()&&setTimeout((()=>{a||(clearInterval(d),showFinalAlert(errStr,domain))}),3e3)}catch(t){r(t)}}}();");
