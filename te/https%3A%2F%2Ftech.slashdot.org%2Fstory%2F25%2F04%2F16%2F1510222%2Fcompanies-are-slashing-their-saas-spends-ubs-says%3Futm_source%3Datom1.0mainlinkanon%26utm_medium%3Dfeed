    <script>
    if (window.is_euro_union === 0 && !bizx.cmp.show_do_not_sell()) {
        var current_time = (new Date()).getTime();
        var image_url = 'https://io.narrative.io/?companyId=2440&id=site_name:slashdot.org&id=url:' + window.location.pathname + '&rand=' + current_time;
