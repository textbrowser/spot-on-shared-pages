  // Load the interlude player.
  window.loadInterludePlayer = function() {
    const src = 'https://player.cnevids.com/interlude/arstechnica.js';

    // Test that script isn't already loaded.
    if (document.querySelector('script[src="' + src + '"]')) {
      return;
    }

    const s = document.createElement('script');
    s.setAttribute('async', true);
    s.setAttribute('src', src);
    document.body.appendChild(s);
  };

